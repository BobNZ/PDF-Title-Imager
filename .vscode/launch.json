{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Default",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": []
        } ,
        {
            "name": "Recursive",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--recursive"]
        },
        {
            "name": "Recursive Summary",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--recursive","--summary"], 
        },
        {
            "name": "Recursive Forced",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--recursive", "--force"]
        }
        {
            "name": "High Quality",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--recursive", "--quality","80"]
        }
    ]
}